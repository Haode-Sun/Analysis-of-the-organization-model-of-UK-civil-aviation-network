@article{FreemanLintonC.1979Cisn,
abstract = {The intuitive background for measures of structural centrality in social networks is reviewed and existing measures are evaluated in terms of their consistency with intuitions and their interpretability.
Three distinct intuitive conceptions of centrality are uncovered and existing measures are refined to embody these conceptions. Three measures are developed for each concept, one absolute and one relative measure of the centrality of positions in a network, and one reflecting the degree of centralization of the entire network. The implications of these measures for the experimental study of small groups is examined.},
author = {Freeman, Linton C.},
address = {AMSTERDAM},
copyright = {1978},
issn = {0378-8733},
journal = {Social networks},
keywords = {Anthropology ; Centrality ; Intuition ; Life Sciences & Biomedicine ; Measure/Measures/Measuring/ Measurement ; Science & Technology ; Social network/Social networks/Social networking ; Social Sciences ; Sociology},
